<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/linearlayout_boder1"
        android:orientation="vertical"
        android:focusable="true"
        android:focusableInTouchMode="true">


        <LinearLayout
            android:id="@+id/housefrom"
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearheightbig"
            android:visibility="gone"
            android:background="@drawable/linearlayout_boder1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textColor="@color/textcolor"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:text="房屋来源："/>
            <TextView
                android:id="@+id/fangwulaiyuan"
                android:tag="ddlFangWuLaiYuan"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:drawableRight="@drawable/ep"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/editbig2"
                android:layout_marginBottom="@dimen/editbig2"
                android:background="@drawable/bg_tv"
                android:gravity="center"
                android:text="自有"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="0.5"
                android:text="*"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearheightbig"
            android:background="@drawable/linearlayout_boder1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin"
                android:layout_weight="1"
                android:textColor="@color/textcolor"
                android:text="省："/>
            <TextView
                android:id="@+id/nowsheng"
                android:tag="ddlStuAddressP"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:drawableRight="@drawable/ep"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/editbig2"
                android:layout_marginBottom="@dimen/editbig2"
                android:background="@drawable/bg_tv"
                android:gravity="center"
                android:text="山东省"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="0.5"
                android:text="*"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearheightbig"
            android:background="@drawable/linearlayout_boder1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin"
                android:layout_weight="1"
                android:textColor="@color/textcolor"
                android:text="市："/>
            <TextView
                android:id="@+id/nowshi"
                android:tag="ddlStuAddressC"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:drawableRight="@drawable/ep"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/editbig2"
                android:layout_marginBottom="@dimen/editbig2"
                android:background="@drawable/bg_tv"
                android:gravity="center"
                android:text="青岛市"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="0.5"
                android:text="*"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearheightbig"
            android:background="@drawable/linearlayout_boder1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin"
                android:layout_weight="1"
                android:textColor="@color/textcolor"
                android:text="县/区："/>
            <TextView
                android:id="@+id/nowxian"
                android:tag="ddlStuAddressA"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:drawableRight="@drawable/ep"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/editbig2"
                android:layout_marginBottom="@dimen/editbig2"
                android:background="@drawable/bg_tv"
                android:gravity="center"
                android:text="市北区"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="0.5"
                android:text="*"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearheightbig"
            android:background="@drawable/linearlayout_boder1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin"
                android:layout_weight="1"
                android:textColor="@color/textcolor"
                android:text="是否特殊地址："/>
            <RadioGroup
                android:id="@+id/nowteshurg"
                android:tag="IsTeShuAddr"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:drawableRight="@drawable/ep"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/editbig2"
                android:layout_marginBottom="@dimen/editbig2"
                android:background="@drawable/bg_tv"
                android:gravity="center"
                android:orientation="horizontal"
                android:text="－请选择－">
                <RadioButton
                    android:id="@+id/nshi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="是"/>
                <RadioButton
                    android:id="@+id/nfou"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:text="否"/>
            </RadioGroup>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="0.5"
                android:text="*"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/putonglinear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/linearheightbig"
                android:background="@drawable/linearlayout_boder1">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_weight="1"
                    android:textColor="@color/textcolor"

                    android:text="道路/小区："/>
                <TextView
                    android:id="@+id/nowjiedao"
                    android:tag="ddlRoadName"
                    android:layout_width="0dp"
                    android:layout_weight="3"
                    android:drawableRight="@drawable/ep"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/editbig2"
                    android:layout_marginBottom="@dimen/editbig2"
                    android:background="@drawable/bg_tv"
                    android:gravity="center"
                    android:text="－请选择－"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="0.5"
                    android:text="*"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/linearheightbig"
                android:background="@drawable/linearlayout_boder1">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_weight="1"
                    android:textColor="@color/textcolor"
                    android:text="门牌号："/>
                <EditText
                    android:id="@+id/nowmenpaihao"
                    android:tag="nbxDoorplateNum"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext"
                    android:layout_marginTop="@dimen/editbig2"
                    android:layout_marginBottom="@dimen/editbig2"
                    android:layout_weight="3"></EditText>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="0.5"
                    android:text="*"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/linearheightbig"
                android:background="@drawable/linearlayout_boder1"
                android:gravity="center">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_weight="1"
                    android:textColor="@color/textcolor"
                    android:text="附号："/>
                <TextView
                    android:id="@+id/nowfuhao"
                    android:tag="ddlDoorplateMarkNum"
                    android:layout_width="0dp"
                    android:layout_weight="3"
                    android:drawableRight="@drawable/ep"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/editbig2"
                    android:layout_marginBottom="@dimen/editbig2"
                    android:background="@drawable/bg_tv"
                    android:gravity="center"
                    android:text="－请选择－"/>
                <ScrollView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text=""/>

                </ScrollView>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/linearlayout_boder1"
                android:paddingLeft="@dimen/activity_vertical_margin"
                android:textSize="12dp"
                android:text="没有选无,如果门牌号带附号则一定要选择.否则门牌号不完整,匹配不到准确的学校."/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/linearheightbig"
                android:background="@drawable/linearlayout_boder1">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_weight="1"
                    android:textColor="@color/textcolor"
                    android:text="楼号："/>
                <EditText
                    android:id="@+id/nowlouhao"
                    android:tag="nbxTowerNum"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext"
                    android:layout_marginTop="@dimen/editbig2"
                    android:layout_marginBottom="@dimen/editbig2"
                    android:layout_weight="3"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="0.5"
                    android:text=""/>

            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/linearlayout_boder1"
                android:paddingLeft="@dimen/activity_vertical_margin"
                android:textSize="12dp"
                android:text="只能填写数字,如果没有可不填，但如果有的话一定要填写,部分学区匹配学校会精确到楼号和单元号.楼号不是数字的请点击特殊地址选择"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/linearheightbig"
                android:background="@drawable/linearlayout_boder1">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_weight="1"
                    android:textColor="@color/textcolor"
                    android:text="单元："/>
                <EditText
                    android:id="@+id/nowdanyuan"
                    android:tag="nbxDanYuanNum"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext"
                    android:layout_marginTop="@dimen/editbig2"
                    android:layout_marginBottom="@dimen/editbig2"
                    android:layout_weight="3"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="0.5"
                    android:text=""/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/linearlayout_boder1"
                android:paddingLeft="@dimen/activity_vertical_margin"
                android:textSize="12dp"
                android:text="只能填写数字,如果没有可不填，但如果有的话一定要填写,部分学区匹配学校会精确到楼号和单元号.单元号不是数字的请点击特殊地址选择"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/linearheightbig"
                android:background="@drawable/linearlayout_boder1">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_weight="1"
                    android:textColor="@color/textcolor"
                    android:text="户号："/>
                <EditText
                    android:id="@+id/nowhuhao"
                    android:tag="nbxHuNum"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext"
                    android:layout_marginTop="@dimen/editbig2"
                    android:layout_marginBottom="@dimen/editbig2"
                    android:layout_weight="3"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="0.5"
                    android:text=""/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/linearlayout_boder1"
                android:paddingLeft="@dimen/activity_vertical_margin"
                android:textSize="12dp"
                android:text="只能填写数字,如果没有可不填,户号不是数字的请点击特殊地址选择"/>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/teshulinear"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/linearheightbig"
            android:background="@drawable/linearlayout_boder1">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin"
                android:layout_weight="1"
                android:textColor="@color/textcolor"
                android:text="特殊地址："/>
            <TextView
                android:id="@+id/nowteshu"
                android:tag="ddlTeShuAddr"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:drawableRight="@drawable/ep"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/editbig2"
                android:layout_marginBottom="@dimen/editbig2"
                android:background="@drawable/bg_tv"
                android:gravity="center"
                android:text="－请选择－"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="0.5"
                android:text="*"/>
        </LinearLayout>



        <Button
            android:background="@drawable/btn_bk4"
            android:id="@+id/next"
            android:layout_marginLeft="@dimen/activity_vertical_margin"
            android:layout_marginRight="@dimen/activity_vertical_margin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="下一步"></Button>
        <View
            android:id="@+id/vMasker"
            android:background="#e0000000"
            android:layout_width="fill_parent"
            android:visibility="gone"
            android:layout_height="100dp"/>
    </LinearLayout>
</ScrollView>
